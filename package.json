{
  "name": "task_management-system",
  "version": "1.0.0",
  "description": "Task Management System",
  "main": "app.js",
  "scripts": {
    "prebuild": "rimraf server/dist && rimraf public/dist",
    "build:prod": "NODE_ENV=production npm run prebuild && npm run build:prod-client && nest build ",
    "format": "prettier --write \"./server/src/**/*.ts\" \"./server/test/**/*.ts\"  \"./public/**/*.{js,jsx}\"",
    "build:dev-client": "webpack serve --open --config ./webpack/webpack.config.js --host 0.0.0.0",
    "build:prod-client": "webpack --progress --profile --config ./webpack/webpack.prod.config.js",
    "start": "concurrently --kill-others \"nest start\" \"npm run build:dev-client\"",
    "start:dev": "concurrently --kill-others \"nest start --watch\" \"npm run build:dev-client\"",
    "start:debug": "concurrently --kill-others \"nest start --debug --watch\" \"npm run build:dev-client\"",
    "start:prod": "NODE_ENV=production node server/dist/main",
    "lint": "eslint \"server/{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./server/test/jest-e2e.json",
    "stats": "webpack --mode production --json > stats.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
